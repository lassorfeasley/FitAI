{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ],
      "components/*": [
        "components/*"
      ],
      "~assets/*": [
        "src/assets/*"
      ],
      "~components/*": [
        "src/components/*"
      ],
      "~pages/*": [
        "src/pages/*"
      ],
      "~styles/*": [
        "src/styles/*"
      ],
      "~/*": [
        "src/*"
      ]
    }
  }
}